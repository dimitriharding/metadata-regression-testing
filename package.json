{
  "name": "metadata-regression-testing",
  "version": "1.2.1",
  "description": "🔄 Metadata Regression Testing Tool 🔄",
  "main": "index.js",
  "bin": {
    "mrt": "./cli.js"
  },
  "scripts": {
    "pretest": "xo --fix",
    "commit": "git-cz",
    "test": "xo && nyc --reporter=html --reporter=text ava -s",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dimitriharding/metadata-regression-testing.git"
  },
  "keywords": [
    "metadata",
    "regression",
    "testing",
    "html",
    "meta",
    "tag"
  ],
  "engines": {
    "node": ">=6.2.0"
  },
  "author": "“Dimitri Harding <“dvhharding@gmail.com”>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dimitriharding/metadata-regression-testing/issues"
  },
  "homepage": "https://github.com/dimitriharding/metadata-regression-testing#readme",
  "dependencies": {
    "chalk": "^2.4.1",
    "fs-extra": "^5.0.0",
    "html-metadata": "^1.6.3",
    "json-diff": "^0.5.2",
    "meow": "^4.0.0",
    "moment": "^2.22.2",
    "pug": "^2.0.3",
    "xa": "^1.0.6"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "browser-sync": "^2.24.4",
    "coveralls": "^3.0.0",
    "execa": "^0.10.0",
    "gulp": "^3.9.1",
    "gulp-bundle-assets": "^2.29.0",
    "gulp-data": "^1.3.1",
    "gulp-pug": "^4.0.1",
    "gulp-rimraf": "^0.2.2",
    "gulp-uglify": "^3.0.0",
    "node-static": "^0.7.10",
    "nyc": "^11.7.3",
    "rmdir": "^1.2.0",
    "semantic-release": "^12.4.1",
    "semantic-release-tamia": "^2.0.1",
    "travis-deploy-once": "^5.0.0",
    "xo": "^0.21.1"
  },
  "xo": {
    "ignores": [
      "diffboard/**/{src,public,bundle.config.js}/**"
    ],
    "rules": {
      "import/no-unassigned-import": 0
    }
  },
  "release": {
    "analyzeCommits": "semantic-release-tamia/analyzeCommits",
    "generateNotes": "semantic-release-tamia/generateNotes",
    "verifyRelease": "semantic-release-tamia/verifyRelease"
  }
}
